  <Subject Name="TalkToAmy_1" UseSingleRandomLine="False" SceneId="7" EffectId="-1">
   <Line CharacterId="1" Direction="concerned" Prio="0">
    <Sound Text="Hey, are you...?" Volume="0.4" FileName="dialog_talktoamy_1_001_player_001" Timestamp="2014-12-04 01:29" />
   </Line>
   <Line CharacterId="3" Direction="pleading" Prio="0">
    <Sound Text="Don't hurt me." FileName="dialog_talktoamy_1_002_amycorpse_001" Timestamp="2014-12-09 11:34" />
   </Line>
  </Subject>
  <Subject Name="TalkToAmy_2" UseSingleRandomLine="False" SceneId="7" EffectId="-1">
   <Line CharacterId="1" Direction="careful" Prio="0">
    <Sound Text="Can I help?" Volume="0.7" FileName="dialog_talktoamy_2_001_player_001" Timestamp="2014-12-04 01:29" />
   </Line>
   <Line CharacterId="3" Prio="0">
    <Sound Text="It won't let me die. Nothing is allowed to die." VoiceOffset="0.35" ReverbVolume="0.975" FileName="dialog_talktoamy_2_002_amycorpse_001" Timestamp="2015-04-08 04:35" />
   </Line>
  </Subject>
  <Subject Name="TalkToAmy_3" UseSingleRandomLine="False" SceneId="7" EffectId="-1">
   <Line CharacterId="1" Prio="0">
    <Sound Text="What happened?" Volume="0.7" FileName="dialog_talktoamy_3_001_player_001" Timestamp="2014-12-04 01:29" />
   </Line>
   <Line CharacterId="3" Prio="0">
    <Sound Text="An accident... fixing the power. I was going to Theta." Volume="0.7" FileName="dialog_talktoamy_3_002_amycorpse_001" Timestamp="2014-12-04 01:29" />
   </Line>
  </Subject>
  <Subject Name="TalkToAmy_CablePulled_1" UseSingleRandomLine="False" SceneId="7" EffectId="-1">
   <Line CharacterId="1" Direction="I'm sorry!" Prio="0">
    <Sound Text="Are you okay?" FileName="dialog_talktoamy_cablepulled_1_001_player_001" Timestamp="2014-12-12 04:02" />
   </Line>
   <Line CharacterId="3" Direction="dead tired" Prio="0">
    <Sound Text="I don't know. I want to go home." Volume="0.65" FileName="dialog_talktoamy_cablepulled_1_002_amycorpse_001" Timestamp="2014-12-04 01:29" />
   </Line>
  </Subject>
  <Subject Name="TalkToAmy_4" UseSingleRandomLine="False" SceneId="7" EffectId="-1">
   <Line CharacterId="1" Prio="0">
    <Sound Text="Anything I can do?" Volume="0.4" FileName="dialog_talktoamy_4_001_player_001" Timestamp="2014-12-12 04:02" />
   </Line>
   <Line CharacterId="3" Prio="0">
    <Sound Text="If you see Masters or Holland, tell them I need help, okay? Amy needs help." Volume="0.7" FileName="dialog_talktoamy_4_002_amycorpse_001" Timestamp="2014-12-04 01:29" />
   </Line>
  </Subject>
  <Subject Name="TalkToAmy_Final_1" UseSingleRandomLine="False" SceneId="7" EffectId="-1">
   <Line CharacterId="1" Prio="0">
    <Sound Text="Just hang in there." FileName="dialog_talktoamy_final_1_001_player_001" Timestamp="2014-12-11 03:12" />
   </Line>
   <Line CharacterId="3" Direction="tired" Prio="0">
    <Sound Text="I'll do my best." VoiceOffset="0.65" FileName="dialog_talktoamy_final_1_002_amycorpse_001" Timestamp="2014-12-12 12:18" />
   </Line>
  </Subject>
  <Subject Name="TalkToAmy_Final_2" UseSingleRandomLine="False" SceneId="7" EffectId="-1">
   <Line CharacterId="1" Prio="0">
    <Sound Text="I'll find someone who can help." Volume="0.75" FileName="dialog_talktoamy_final_2_001_player_001" Timestamp="2014-12-12 12:18" />
   </Line>
   <Line CharacterId="3" Direction="tired" Prio="0">
    <Sound Text="Roger that." FileName="dialog_talktoamy_final_2_002_amycorpse_001" Timestamp="2014-12-12 12:18" />
   </Line>
  </Subject>
  <Subject Name="TalkToAmy_Final_3" UseSingleRandomLine="False" SceneId="7" EffectId="-1">
   <Line CharacterId="1" Prio="0">
    <Sound Text="You'll be fine." Volume="0.5" FileName="dialog_talktoamy_final_3_001_player_001" Timestamp="2014-12-11 03:12" />
   </Line>
   <Line CharacterId="3" Direction="tired" Prio="0">
    <Sound Text="Thank you." FileName="dialog_talktoamy_final_3_002_amycorpse_001" Timestamp="2014-12-12 12:18" />
   </Line>
  </Subject>
  <Subject Name="TalkToAmy_Final_4" UseSingleRandomLine="False" SceneId="7" EffectId="-1">
   <Line CharacterId="1" Prio="0">
    <Sound Text="They'll patch you right up." Timestamp="2014-12-11 03:12" />
   </Line>
   <Line CharacterId="3" Direction="tired" Prio="0">
    <Sound Text="Hope so." VoiceOffset="0.65" Volume="0.7" Timestamp="2014-12-12 12:18" />
   </Line>
  </Subject>
  <Subject Name="TalkToAmy_CablePulled_Final_1" UseSingleRandomLine="False" SceneId="7" EffectId="-1">
   <Line CharacterId="1" Prio="0">
    <Sound Text="Don't worry, it's going to be all right." Volume="0.4" FileName="dialog_talktoamy_cablepulled_final_1_001_player_001" Timestamp="2014-12-12 12:18" />
   </Line>
   <Line CharacterId="3" Direction="sad, scared" Prio="0">
    <Sound Text="Okay." Volume="0.5" FileName="dialog_talktoamy_cablepulled_final_1_002_amycorpse_001" Timestamp="2014-12-12 12:18" />
   </Line>
  </Subject>
  <Subject Name="TalkToAmy_CablePulled_Final_2" UseSingleRandomLine="False" SceneId="7" EffectId="-1">
   <Line CharacterId="1" Prio="0">
    <Sound Text="You got nothing to worry about." Volume="0.4" FileName="dialog_talktoamy_cablepulled_final_2_001_player_001" Timestamp="2015-04-08 04:35" />
   </Line>
   <Line CharacterId="3" Direction="sad, scared" Prio="0">
    <Sound Text="Okay." FileName="dialog_talktoamy_cablepulled_final_2_002_amycorpse_001" Timestamp="2014-12-12 12:18" />
   </Line>
  </Subject>
  <Subject Name="TalkToAmy_CablePulled_Final_3" UseSingleRandomLine="False" SceneId="7" EffectId="-1">
   <Line CharacterId="1" Prio="0">
    <Sound Text="Everything's going to be fine." Volume="0.45" FileName="dialog_talktoamy_cablepulled_final_3_001_player_001" Timestamp="2014-12-11 03:12" />
   </Line>
   <Line CharacterId="3" Direction="sad, scared" Prio="0">
    <Sound Text="Okay." Volume="0.6" FileName="dialog_talktoamy_cablepulled_final_3_002_amycorpse_001" Timestamp="2014-12-12 12:18" />
   </Line>
