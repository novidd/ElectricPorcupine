  <Subject Name="LambdaTransmissionLink" UseSingleRandomLine="False" SceneId="0" EffectId="-1">
   <Line CharacterId="0" Direction="worried" Prio="0" Callback="LineStart_CommConversation">
    <Sound Text="Simon... Simon? Are you there?" ExtraEffectFile="Flashack/Level/01_04/cath_radio_complex" EndPadding="-0.3" Volume="0.7" FileName="radio_lambdatransmissionlink_001_catherine_001" Timestamp="2015-06-01 11:29" />
   </Line>
   <Line CharacterId="1" Direction="shocked, angry" Prio="0">
    <Sound Text="Catherine, is that you? I was on my way, but then the shuttle train... it fucking crashed." EndPadding="-0.1" FileName="radio_lambdatransmissionlink_002_player_001" Timestamp="2015-01-09 01:25" />
   </Line>
   <Line CharacterId="0" Direction="worried" Prio="0">
    <Sound Text="Are you all right? The system says the section is sealed for a suspected hull breach." EndPadding="-0.1" Volume="0.9" FileName="radio_lambdatransmissionlink_003_catherine_001" Timestamp="2015-05-21 02:50" />
   </Line>
   <Line CharacterId="1" Direction="interrupt" Prio="0">
    <Sound Text="No, I'm not all right! What the hell happened to the world? Why are we under water?" EndPadding="-0.4" FileName="radio_lambdatransmissionlink_004_player_001" Timestamp="2015-01-09 01:25" />
   </Line>
   <Line CharacterId="0" Direction="we don't have time for this" Prio="0">
    <Sound Text="Wow, you are really out of place, aren't you?" EndPadding="-0.1" FileName="radio_lambdatransmissionlink_005_catherine_001" Timestamp="2015-01-09 01:36" />
    <Sound Text="Look, don't worry about that right now. I realize you're confused, but you're so close to Lambda. If you'd just keep going." FileName="radio_lambdatransmissionlink_005_catherine_002" Timestamp="2015-04-08 04:35" />
   </Line>
   <Line CharacterId="1" Prio="0">
    <Sound Text="How close? Can I walk there?" FileName="radio_lambdatransmissionlink_006_player_001" Timestamp="2015-01-09 01:25" />
   </Line>
   <Line CharacterId="0" Prio="0">
    <Sound Text="Yeah, but the tunnel is locked off to protect the overall structure from collapsing. What you need to find is your section's maintenance hatch. It will lead outside." Volume="0.8" FileName="radio_lambdatransmissionlink_007_catherine_001" Timestamp="2015-05-21 02:50" />
   </Line>
   <Line CharacterId="1" Prio="0">
    <Sound Text="Maintenance hatch? No problem." FileName="radio_lambdatransmissionlink_008_player_001" Timestamp="2015-01-09 01:36" />
   </Line>
   <Line CharacterId="0" Direction="internal, then quickly signing off" Prio="0">
    <Sound Text="What is that -- what's going on out there?" VoiceOffset="0.4" FileName="radio_lambdatransmissionlink_009_catherine_001" Timestamp="2015-05-21 02:50" />
    <Sound Text="I got to go. See you soon, Simon." VoiceOffset="0.4" FileName="radio_lambdatransmissionlink_009_catherine_002" Timestamp="2015-04-08 04:35" />
   </Line>
   <Line CharacterId="7" Context="Connection busted" Prio="0" Callback="LineOver_InteractTransmissionLink">
    <Sound Text="*crack*" FileName="radio_lambdatransmissionlink_010__soundeffect_001" Timestamp="2015-01-09 01:25" />
   </Line>
   <Line CharacterId="1" Prio="0">
    <Sound Text="Catherine? You okay?" FileName="radio_lambdatransmissionlink_011_player_001" Timestamp="2015-01-09 01:25" />
   </Line>
  </Subject>
