  <Subject Name="2_ConfusedRobotIdling_Damaged_1" UseSingleRandomLine="True" SceneId="5" EffectId="-1">
   <Line CharacterId="16" Direction="shocked" Prio="5" PlayOnce="True" EntitySource="Goya" MaxDist="20" MinFreq="5000" MaxFreq="1250" FrequencyFilter="Low Pass">
    <Sound Text="Brandon. Are you okay? What the hell happened?" FileName="goya_2_confusedrobotidling_damaged_1_001_goya_001" Timestamp="2014-11-10 09:50" />
   </Line>
   <Line CharacterId="16" Prio="4" PlayOnce="True" EntitySource="Goya" MaxDist="20" MinFreq="5000" MaxFreq="1250" FrequencyFilter="Low Pass">
    <Sound Text="I don't get it. What was that?" FileName="goya_2_confusedrobotidling_damaged_1_002_goya_001" Timestamp="2014-11-10 09:50" />
   </Line>
   <Line CharacterId="16" Prio="3" PlayOnce="True" EntitySource="Goya" MaxDist="20" MinFreq="5000" MaxFreq="1250" FrequencyFilter="Low Pass">
    <Sound Text="Stay down, Brandon. Not sure it's safe." FileName="goya_2_confusedrobotidling_damaged_1_003_goya_001" Timestamp="2014-11-10 09:50" />
   </Line>
   <Line CharacterId="16" Prio="2" PlayOnce="True" EntitySource="Goya" MaxDist="20" MinFreq="5000" MaxFreq="1250" FrequencyFilter="Low Pass">
    <Sound Text="Christ. This place is crazy." FileName="goya_2_confusedrobotidling_damaged_1_004_goya_001" Timestamp="2015-04-09 01:30" />
   </Line>
   <Line CharacterId="16" Prio="1" PlayOnce="True" EntitySource="Goya" MaxDist="20" MinFreq="5000" MaxFreq="1250" FrequencyFilter="Low Pass">
    <Sound Text="Akers! Did you see that?!" FileName="goya_2_confusedrobotidling_damaged_1_005_goya_001" Timestamp="2014-11-10 09:50" />
   </Line>
   <Line CharacterId="16" Prio="0" EntitySource="Goya" MaxDist="20" MinFreq="5000" MaxFreq="1250" FrequencyFilter="Low Pass">
    <Sound Text="That really hurt." FileName="goya_2_confusedrobotidling_damaged_1_006_goya_001" Timestamp="2014-11-10 09:50" />
   </Line>
   <Line CharacterId="16" Prio="0" EntitySource="Goya" MaxDist="20" MinFreq="5000" MaxFreq="1250" FrequencyFilter="Low Pass">
    <Sound Text="Came out of nowhere." FileName="goya_2_confusedrobotidling_damaged_1_007_goya_001" Timestamp="2014-11-10 09:50" />
   </Line>
   <Line CharacterId="16" Prio="0" ChangeSource="True" EntitySource="Goya" MaxDist="20" MinFreq="5000" MaxFreq="1250" FrequencyFilter="Low Pass">
    <Sound Text="Got to be more careful." FileName="goya_2_confusedrobotidling_damaged_1_008_goya_001" Timestamp="2014-11-10 09:50" />
   </Line>
  </Subject>
  <Subject Name="2_ConfusedRobotIdling_Damaged_2" UseSingleRandomLine="True" SceneId="5" EffectId="-1">
   <Line CharacterId="16" Direction="scared" Prio="5" PlayOnce="True" EntitySource="Goya" MaxDist="25" MinFreq="7000" MaxFreq="1000" FrequencyFilter="Low Pass">
    <Sound Text="Guys, I need help!" FileName="goya_2_confusedrobotidling_damaged_2_001_goya_001" Timestamp="2014-11-10 09:50" />
   </Line>
   <Line CharacterId="16" Prio="4" PlayOnce="True" EntitySource="Goya" MaxDist="25" MinFreq="7000" MaxFreq="1000" FrequencyFilter="Low Pass">
    <Sound Text="Brandon?! Come on, help me out!" FileName="goya_2_confusedrobotidling_damaged_2_002_goya_001" Timestamp="2014-11-10 09:50" />
   </Line>
   <Line CharacterId="16" Prio="3" PlayOnce="True" EntitySource="Goya" MaxDist="25" MinFreq="7000" MaxFreq="1000" FrequencyFilter="Low Pass">
    <Sound Text="Shit, the suit is filling up with water." FileName="goya_2_confusedrobotidling_damaged_2_003_goya_001" Timestamp="2014-11-10 09:50" />
   </Line>
   <Line CharacterId="16" Prio="2" PlayOnce="True" EntitySource="Goya" MaxDist="25" MinFreq="7000" MaxFreq="1000" FrequencyFilter="Low Pass">
    <Sound Text="Ah god, there's a lot of blood." FileName="goya_2_confusedrobotidling_damaged_2_004_goya_001" Timestamp="2014-11-10 09:50" />
   </Line>
   <Line CharacterId="16" Prio="1" PlayOnce="True" EntitySource="Goya" MaxDist="25" MinFreq="7000" MaxFreq="1000" FrequencyFilter="Low Pass">
    <Sound Text="Guys, I'm fucked. I'm in really bad shape." FileName="goya_2_confusedrobotidling_damaged_2_005_goya_001" Timestamp="2014-11-10 09:50" />
   </Line>
   <Line CharacterId="16" Prio="0" EntitySource="Goya" MaxDist="20" MinFreq="5000" MaxFreq="1250" FrequencyFilter="Low Pass">
    <Sound Text="Brandon?! Terry?!" FileName="goya_2_confusedrobotidling_damaged_2_006_goya_001" Timestamp="2014-11-10 09:50" />
   </Line>
   <Line CharacterId="16" Prio="0" EntitySource="Goya" MaxDist="20" MinFreq="5000" MaxFreq="1250" FrequencyFilter="Low Pass">
    <Sound Text="I don't want to die." FileName="goya_2_confusedrobotidling_damaged_2_007_goya_001" Timestamp="2014-11-10 09:50" />
   </Line>
   <Line CharacterId="16" Prio="0" EntitySource="Goya" MaxDist="20" MinFreq="5000" MaxFreq="1250" FrequencyFilter="Low Pass">
    <Sound Text="Why is this happening?" FileName="goya_2_confusedrobotidling_damaged_2_008_goya_001" Timestamp="2014-11-10 09:50" />
   </Line>
