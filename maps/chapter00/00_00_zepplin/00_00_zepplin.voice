 <Subjects>
  <Subject Name="7_ZeppelinTransmission_2" UseSingleRandomLine="False" SceneId="2" EffectId="-1">
   <Line CharacterId="2" Direction="on radio" Prio="0" ChangeSource="True" EntitySource="ZeppelinTerminalSoundSource" MaxDist="8" MinFreq="5000" MaxFreq="3000" FrequencyFilter="Low Pass">
    <Sound Text="Peter, can you hear me?" ExtraEffectFile="Flashback/Level/02_03/falloff/zeppelinTransmission2" EndPadding="-1" Volume="0.75" ReverbVolume="0" FileName="recording_7_zeppelintransmission_2_001_akers_001" Timestamp="2015-01-24 07:51" />
   </Line>
   <Line CharacterId="15" Prio="0" ChangeSource="True" EntitySource="ZeppelinTerminalSoundSource" MaxDist="8" MinFreq="5000" MaxFreq="3000" FrequencyFilter="Low Pass">
    <Sound Text="Uh, I hear you, Terry. I mean Mr. Akers, sir." EndPadding="-0.2" FileName="recording_7_zeppelintransmission_2_002_strasky_001" Timestamp="2015-05-21 06:03" />
   </Line>
   <Line CharacterId="2" Prio="0" ChangeSource="True" EntitySource="ZeppelinTerminalSoundSource" MaxDist="8" MinFreq="5000" MaxFreq="3000" FrequencyFilter="Low Pass">
    <Sound Text="I've changed my mind. I would like to come to Theta. Let Delta sleep." Volume="0.75" ReverbVolume="0" FileName="recording_7_zeppelintransmission_2_003_akers_001" Timestamp="2015-01-24 07:51" />
   </Line>
   <Line CharacterId="15" Prio="0" ChangeSource="True" EntitySource="ZeppelinTerminalSoundSource" MaxDist="8" MinFreq="5000" MaxFreq="3000" FrequencyFilter="Low Pass">
    <Sound Text="Of course! Wow, honestly didn't expect to hear from you again. Such a long time since you checked in." VoiceOffset="" FileName="recording_7_zeppelintransmission_2_004_strasky_001" Timestamp="2015-01-27 01:51" />
    <Sound Text="We have a few divers in a zeppelin surveying Lambda at moment. I'll just make sure they pick you up when they're done." FileName="recording_7_zeppelintransmission_2_004_strasky_002" Timestamp="2015-01-24 07:51" />
   </Line>
   <Line CharacterId="2" Direction="laughing" Prio="0" ChangeSource="True" EntitySource="ZeppelinTerminalSoundSource" MaxDist="8" MinFreq="5000" MaxFreq="3000" FrequencyFilter="Low Pass">
    <Sound Text="Ah, thank you for the warning. I'll make sure they feel welcome." Volume="0.75" FileName="recording_7_zeppelintransmission_2_005_akers_001" Timestamp="2015-05-21 06:03" />
   </Line>
   <Line CharacterId="15" Direction="awkward laugh" Prio="0" ChangeSource="True" EntitySource="ZeppelinTerminalSoundSource" MaxDist="8" MinFreq="5000" MaxFreq="3000" FrequencyFilter="Low Pass">
    <Sound Text="Ok, see you when you get here." FileName="recording_7_zeppelintransmission_2_006_strasky_001" Timestamp="2015-01-24 07:51" />
   </Line>
  </Subject>
  <Subject Name="7_ZeppelinTransmission_1" UseSingleRandomLine="False" SceneId="2" EffectId="-1">
   <Line CharacterId="17" Prio="0" ChangeSource="True" EntitySource="ZeppelinTerminalSoundSource" MaxDist="8" MinFreq="5000" MaxFreq="3000" FrequencyFilter="Low Pass">
    <Sound Text="Strasky? We can see the zeppelin now." ExtraEffectFile="Flashback/Level/02_03/falloff/zeppelinTransmission1" EndPadding="-0.5" Volume="0.75" ReverbVolume="0" FileName="recording_7_zeppelintransmission_1_001_wan_001" Timestamp="2015-01-30 09:33" />
   </Line>
   <Line CharacterId="15" Prio="0" ChangeSource="True" EntitySource="ZeppelinTerminalSoundSource" MaxDist="8" MinFreq="5000" MaxFreq="3000" FrequencyFilter="Low Pass">
    <Sound Text="Good. Sorry about the mess. Without the positioning system, the zepps just keep losing their set routes." EndPadding="-0.45" Volume="0.75" ReverbVolume="0" FileName="recording_7_zeppelintransmission_1_002_strasky_001" Timestamp="2015-01-24 07:51" />
    <Sound Text="Wolchezk did say that you could have fixed it on your side by re-jigging the antenna." EndPadding="-0.25" Volume="0.75" ReverbVolume="0" FileName="recording_7_zeppelintransmission_1_002_strasky_002" Timestamp="2015-01-24 07:51" />
    <Sound Text="Guess it doesn't matter now. Are all of you coming?" EndPadding="-0.42" Volume="0.75" ReverbVolume="0" FileName="recording_7_zeppelintransmission_1_002_strasky_003" Timestamp="2015-01-24 07:51" />
   </Line>
   <Line CharacterId="17" Prio="0" ChangeSource="True" EntitySource="ZeppelinTerminalSoundSource" MaxDist="8" MinFreq="5000" MaxFreq="3000" FrequencyFilter="Low Pass">
    <Sound Text="No, just me, Goya, and Krier. Akers is set on being a stubborn old man." EndPadding="-0.9" Volume="0.75" ReverbVolume="0" FileName="recording_7_zeppelintransmission_1_003_wan_001" Timestamp="2015-01-24 07:51" />
   </Line>
   <Line CharacterId="15" Direction="laughs" Prio="0" ChangeSource="True" EntitySource="ZeppelinTerminalSoundSource" MaxDist="8" MinFreq="5000" MaxFreq="3000" FrequencyFilter="Low Pass">
    <Sound Text="All right. See you soon, Brandon. Happy evac!" EndPadding="-0.4" FileName="recording_7_zeppelintransmission_1_004_strasky_001" Timestamp="2015-01-24 07:51" />
   </Line>
  </Subject>
  <Subject Name="2_DatamineMaggie" UseSingleRandomLine="False" SceneId="3" EffectId="-1">
   <Line CharacterId="4" Direction="exhausted, dying" Prio="0" ChangeSource="True" EntitySource="Player">
    <Sound Text="Come in, Strasky. Can you hear me?" ExtraEffectFile="Flashback/Level/02_03/dataminemaggie" EndPadding="-1.4" Volume="0.6" FileName="datamine_2_dataminemaggie_003_maggie_001" Timestamp="2015-01-27 03:55" />
   </Line>
   <Line CharacterId="4" Direction="exhausted, dying" Prio="0" ChangeSource="True" EntitySource="Player">
    <Sound Text="Theta, listen! Akers is headed for Theta!" EndPadding="-2.4" Volume="0.85" FileName="datamine_2_dataminemaggie_004_maggie_001" Timestamp="2015-01-27 03:55" />
   </Line>
   <Line CharacterId="18" Direction="unaware of the excitement" Prio="0" ChangeSource="True" EntitySource="Player">
    <Sound Text="Delta LUMAR Annex, getting a signal, but no audio." EndPadding="-0.6" FileName="datamine_2_dataminemaggie_005_strasky_001" Timestamp="2015-01-27 03:55" />
   </Line>
   <Line CharacterId="4" Direction="exhausted, dying" Prio="0" ChangeSource="True" EntitySource="Player">
    <Sound Text="Strasky! It's Komorebi! Akers has gone mad -- he's dangerous!" EndPadding="-1.9" Volume="0.85" FileName="datamine_2_dataminemaggie_006_maggie_001" Timestamp="2015-05-21 06:03" />
   </Line>
   <Line CharacterId="4" Direction="exhausted, dying" Prio="0" ChangeSource="True" EntitySource="Player">
    <Sound Text="Strasky?!" EndPadding="0.7" Volume="0.85" FileName="datamine_2_dataminemaggie_007_maggie_001" Timestamp="2015-01-27 03:55" />
   </Line>
   <Line CharacterId="18" Prio="0" ChangeSource="True" EntitySource="Player">
    <Sound Text="Sorry, Delta. Still no audio -- try calling from another access point." EndPadding="-0.9" FileName="datamine_2_dataminemaggie_008_strasky_001" Timestamp="2015-05-21 06:03" />
   </Line>
   <Line CharacterId="4" Direction="exhausted, dying" Prio="0" ChangeSource="True" EntitySource="Player">
    <Sound Text="Strasky, I can try sending... I have pictures..." EndPadding="-1" Volume="0.85" EndsAfterExtraEffect="True" FileName="datamine_2_dataminemaggie_009_maggie_001" Timestamp="2015-04-09 01:30" />
   </Line>
  </Subject>
