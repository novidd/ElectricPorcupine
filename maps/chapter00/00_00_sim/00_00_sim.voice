  <Subject Name="3_IncorrectScanUpload" UseSingleRandomLine="True" SceneId="1" EffectId="-1">
   <Line CharacterId="1" Prio="4" PlayOnce="True" Rerecord="True">
    <Sound Text="No, won't work. We need someone from Theta Security Group." FileName="dialog_3_incorrectscanupload_001_catherine_001" Timestamp="2015-05-28 03:58" />
   </Line>
   <Line CharacterId="1" Prio="3" PlayOnce="True" Rerecord="True">
    <Sound Text="That one won't work either. Look for security people like Cronstedt or Strohmeier." FileName="dialog_3_incorrectscanupload_002_catherine_001" Timestamp="2015-05-07 02:49" />
   </Line>
   <Line CharacterId="1" Prio="2" PlayOnce="True" Rerecord="True">
    <Sound Text="No, I don't think so. Strohmeier recruited people from the evacuated sites. They should also know the cipher, if you can find one of them." FileName="dialog_3_incorrectscanupload_003_catherine_001" Timestamp="2015-05-21 09:29" />
   </Line>
   <Line CharacterId="1" Prio="1" PlayOnce="True">
    <Sound Text="Okay, try actually reading the info and then pick one." FileName="dialog_3_incorrectscanupload_004_catherine_001" Timestamp="2014-04-29 12:46" />
   </Line>
   <Line CharacterId="1" Prio="0">
    <Sound Text="*disappointed sigh*" FileName="dialog_3_incorrectscanupload_005_catherine_001" Timestamp="2014-02-04 05:10" />
   </Line>
   <Line CharacterId="1" Prio="0">
    <Sound Text="*hmpf*" FileName="dialog_3_incorrectscanupload_006_catherine_001" Timestamp="2014-02-04 05:10" />
   </Line>
   <Line CharacterId="1" Prio="0">
    <Sound Text="*frustrated sigh*" FileName="dialog_3_incorrectscanupload_007_catherine_001" Timestamp="2014-02-04 05:10" />
   </Line>
  </Subject>
