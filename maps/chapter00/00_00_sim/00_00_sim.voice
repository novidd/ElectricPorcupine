   </Line>
  </Subject>
  <Subject Name="4_FixedServersAndLeftArea" UseSingleRandomLine="False" SceneId="1" EffectId="-1">
   <Line CharacterId="1" Prio="0">
    <Sound Text="Good job, Simon." Volume="0.95" FileName="dialog_4_fixedserversandleftarea_001_catherine_001" Timestamp="2015-01-13 02:20" />
   </Line>
   <Line CharacterId="0" Prio="0">
    <Sound Text="This better have worked. I'm not going down there again." EndPadding="-0.2" Volume="0.7" FileName="dialog_4_fixedserversandleftarea_002_player_001" Timestamp="2015-04-09 03:43" />
   </Line>
  </Subject>
