  <Subject Name="3_Cath_ReadCometArticle" UseSingleRandomLine="False" SceneId="9" EffectId="-1">
   <Line CharacterId="7" Prio="0">
    <Sound Text="There's really no way around this -- a freaking comet killed the planet." FileName="cathandsimon_3_cath_readcometarticle_001_player_001" Timestamp="2015-05-21 04:03" />
   </Line>
   <Line CharacterId="16" Prio="0">
    <Sound Text="Afraid so." VoiceOffset="0.25" FileName="cathandsimon_3_cath_readcometarticle_002_catherineomnitool_001" Timestamp="2014-11-28 04:35" />
   </Line>
   <Line CharacterId="7" Prio="0">
    <Sound Text="You were all stranded down here." VoiceOffset="0.5" FileName="cathandsimon_3_cath_readcometarticle_003_player_001" Timestamp="2014-11-28 04:35" />
   </Line>
   <Line CharacterId="16" Prio="0">
    <Sound Text="And we were the lucky ones." VoiceOffset="0.25" FileName="cathandsimon_3_cath_readcometarticle_004_catherineomnitool_001" Timestamp="2014-11-28 04:35" />
   </Line>
  </Subject>
  <Subject Name="3_Cath_GlassPoundResponse_Unhurt" UseSingleRandomLine="False" SceneId="9" EffectId="-1">
   <Line CharacterId="16" Prio="0">
    <Sound Text="What are you doing? Be careful." FileName="cathandsimon_3_cath_glasspoundresponse_unhurt_001_catherineomnitool_001" Timestamp="2014-11-07 07:53" />
   </Line>
  </Subject>
  <Subject Name="3_Cath_ReminderReturnAfterTracking" UseSingleRandomLine="True" SceneId="9" EffectId="-1">
   <Line CharacterId="16" Prio="0">
    <Sound Text="All right, let's do this. Come pick me up." FileName="cathandsimon_3_cath_reminderreturnaftertracking_001_catherineomnitool_001" Timestamp="2014-11-07 07:53" />
   </Line>
   <Line CharacterId="16" Prio="0">
    <Sound Text="Don't forget to take me along -- the omnitool." FileName="cathandsimon_3_cath_reminderreturnaftertracking_002_catherineomnitool_001" Timestamp="2015-05-21 04:03" />
   </Line>
   <Line CharacterId="16" Prio="0">
    <Sound Text="Where did you go? I'm waiting." FileName="cathandsimon_3_cath_reminderreturnaftertracking_003_catherineomnitool_001" Timestamp="2015-01-30 09:27" />
   </Line>
  </Subject>
  <Subject Name="2_Cath_PlayerNearCathFirstTime_Hurt" UseSingleRandomLine="True" SceneId="9" EffectId="-1">
   <Line CharacterId="15" Prio="2" PlayOnce="True">
    <Sound Text="There you are. Upright and everything." FileName="cathandsimon_2_cath_playernearcathfirsttime_hurt_001_catherineomnitool_001" Timestamp="2015-01-30 09:27" />
   </Line>
   <Line CharacterId="15" Prio="1" PlayOnce="True">
    <Sound Text="That brute really doesn't play nice with electronics." FileName="cathandsimon_2_cath_playernearcathfirsttime_hurt_002_catherineomnitool_001" Timestamp="2014-11-07 07:53" />
   </Line>
   <Line CharacterId="15" Prio="0">
    <Sound Text="Hello? I'm right here." FileName="cathandsimon_2_cath_playernearcathfirsttime_hurt_003_catherineomnitool_001" Timestamp="2014-11-07 07:53" />
   </Line>
  </Subject>
