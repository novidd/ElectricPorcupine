  <Subject Name="2_Cath_InteractTalk_2_OmniToolIn" UseSingleRandomLine="False" SceneId="9" EffectId="-1">
   <Line CharacterId="15" Prio="0" ChangeSource="True" EntitySource="CortexChipTool" MaxDist="24" MaxFreq="1500" FrequencyFilter="Low Pass">
    <Sound Text="All right, time for plan B. What's plan B?" FileName="cathandsimon_2_cath_interacttalk_2_omnitoolin_001_catherinerobot_001" Timestamp="2015-01-30 09:27" />
    <Sound Text="Oh, the omnitool! The tool you inserted into the terminal." FileName="cathandsimon_2_cath_interacttalk_2_omnitoolin_001_catherinerobot_002" Timestamp="2015-01-19 04:50" />
   </Line>
   <Line CharacterId="7" Prio="0">
    <Sound Text="Oh, yeah. The door opener." FileName="cathandsimon_2_cath_interacttalk_2_omnitoolin_002_player_001" Timestamp="2015-01-19 04:41" />
   </Line>
   <Line CharacterId="15" Prio="0" EntitySource="CortexChipTool" MaxDist="24" MaxFreq="1500" FrequencyFilter="Low Pass">
    <Sound Text="Yeah. You can pull my cortex chip and put it into the omnitool." FileName="cathandsimon_2_cath_interacttalk_2_omnitoolin_003_catherinerobot_001" Timestamp="2015-04-09 10:59" />
   </Line>
   <Line CharacterId="7" Prio="0">
    <Sound Text="What's a cortex chip?" FileName="cathandsimon_2_cath_interacttalk_2_omnitoolin_004_player_001" Timestamp="2014-11-09 08:57" />
   </Line>
   <Line CharacterId="15" Prio="0" EntitySource="CortexChipTool" MaxDist="24" MaxFreq="1500" FrequencyFilter="Low Pass">
    <Sound Text="It will be obvious. I'll eject it for you. Just grab the chip and slide it into the omnitool. Come on, I just need you to do this one thing for me." FileName="cathandsimon_2_cath_interacttalk_2_omnitoolin_005_catherinerobot_001" Timestamp="2015-04-13 01:15" />
   </Line>
   <Line CharacterId="7" Prio="0">
    <Sound Text="Yeah, all right." FileName="cathandsimon_2_cath_interacttalk_2_omnitoolin_006_player_001" Timestamp="2014-11-09 08:57" />
   </Line>
  </Subject>
  <Subject Name="2_Cath_InteractTalk_2_OmniToolOut" UseSingleRandomLine="False" SceneId="9" EffectId="-1">
   <Line CharacterId="15" Prio="0" ChangeSource="True" EntitySource="CortexChipTool" MaxDist="24" MaxFreq="1500" FrequencyFilter="Low Pass">
    <Sound Text="I just need to access the computer. Oh, is that an omnitool you're carrying?" EndPadding="-0.1" Volume="0.7" FileName="cathandsimon_2_cath_interacttalk_2_omnitoolout_001_catherinerobot_001" Timestamp="2015-04-09 10:59" />
   </Line>
   <Line CharacterId="7" Prio="0" Callback="LineOver_Anim_SimonLooksAtOmniTool">
    <Sound Text="Oh, the door opener? I picked it up at Upsilon -- where I woke up." EndPadding="-0.3" FileName="cathandsimon_2_cath_interacttalk_2_omnitoolout_002_player_001" Timestamp="2015-05-21 04:03" />
   </Line>
   <Line CharacterId="15" Prio="0" ChangeSource="True" EntitySource="CortexChipTool" MaxDist="24" MaxFreq="1500" FrequencyFilter="Low Pass">
    <Sound Text="That will have to do. Plug it into the terminal." EndPadding="-0.4" Volume="0.7" FileName="cathandsimon_2_cath_interacttalk_2_omnitoolout_003_catherinerobot_001" Timestamp="2014-11-07 07:53" />
   </Line>
   <Line CharacterId="7" Prio="0">
    <Sound Text="Sure, whatever." FileName="cathandsimon_2_cath_interacttalk_2_omnitoolout_004_player_001" Timestamp="2014-11-07 07:53" />
   </Line>
  </Subject>
  <Subject Name="3_Cath_PutInOmnitToolBeforeTalk" UseSingleRandomLine="False" SceneId="9" EffectId="-1">
   <Line CharacterId="15" Prio="0" ChangeSource="True" EntitySource="CortexChipTool" MaxDist="25" MaxFreq="1500" FrequencyFilter="Low Pass">
    <Sound Text="Oh, you found an omnitool. Very handy." FileName="cathandsimon_3_cath_putinomnittoolbeforetalk_001_catherinerobot_001" Timestamp="2014-11-07 07:53" />
   </Line>
  </Subject>
  <Subject Name="3_Cath_GlassPoundResponse_Hurt" UseSingleRandomLine="False" SceneId="9" EffectId="-1">
   <Line CharacterId="16" Prio="0">
    <Sound Text="You really need to stop drawing attention to yourself." FileName="cathandsimon_3_cath_glasspoundresponse_hurt_001_catherineomnitool_001" Timestamp="2014-11-07 07:53" />
   </Line>
  </Subject>
  <Subject Name="4_Cath_BackToGetOmniTool_2_LifeboatFound" UseSingleRandomLine="False" SceneId="9" EffectId="-1">
   <Line CharacterId="7" Prio="0">
    <Sound Text="You know what, I found a sunken vessel just outside. Bet that could take us to Theta." FileName="cathandsimon_4_cath_backtogetomnitool_2_lifeboatfound_001_player_001" Timestamp="2015-01-19 01:10" />
   </Line>
   <Line CharacterId="16" Prio="0">
    <Sound Text="Really?" FileName="cathandsimon_4_cath_backtogetomnitool_2_lifeboatfound_002_catherineomnitool_001" Timestamp="2014-11-28 05:09" />
   </Line>
   <Line CharacterId="7" Prio="0">
    <Sound Text="Didn't look completely out of action. It even lit up a little when I tried the omnitool on it." FileName="cathandsimon_4_cath_backtogetomnitool_2_lifeboatfound_003_player_001" Timestamp="2015-01-19 01:10" />
   </Line>
   <Line CharacterId="16" Prio="0">
    <Sound Text="Okay then, let's have a look. I'll just eject from this thing. Don't forget to take me with you." FileName="cathandsimon_4_cath_backtogetomnitool_2_lifeboatfound_004_catherineomnitool_001" Timestamp="2015-01-19 01:10" />
   </Line>
  </Subject>
  <Subject Name="4_Cath_BackToGetOmniTool_2_NoLifeboat" UseSingleRandomLine="False" SceneId="9" EffectId="-1">
   <Line CharacterId="7" Prio="0">
    <Sound Text="Maybe there's still a working shuttle train that can take us to Theta." EndPadding="-0.1" FileName="cathandsimon_4_cath_backtogetomnitool_2_nolifeboat_001_player_001" Timestamp="2014-11-28 05:09" />
   </Line>
   <Line CharacterId="16" Prio="0">
    <Sound Text="I doubt it." EndPadding="-0.4" FileName="cathandsimon_4_cath_backtogetomnitool_2_nolifeboat_002_catherineomnitool_001" Timestamp="2014-11-28 05:09" />
   </Line>
   <Line CharacterId="7" Prio="0">
    <Sound Text="Just have a look around. Push comes to shove, I'll walk there." EndPadding="-0.2" FileName="cathandsimon_4_cath_backtogetomnitool_2_nolifeboat_003_player_001" Timestamp="2015-05-21 04:03" />
   </Line>
   <Line CharacterId="16" Direction="hmpf" Prio="0">
    <Sound Text="Okay then. I'll just eject from this thing. Don't forget to take me with you." EndPadding="-0.3" FileName="cathandsimon_4_cath_backtogetomnitool_2_nolifeboat_004_catherineomnitool_001" Timestamp="2015-01-09 05:25" />
   </Line>
   <Line CharacterId="7" Prio="0">
    <Sound Text="I won't forget." FileName="cathandsimon_4_cath_backtogetomnitool_2_nolifeboat_005_player_001" Timestamp="2014-11-28 05:09" />
   </Line>
  </Subject>
  <Subject Name="4_Cath_BackToGetOmniTool_3" UseSingleRandomLine="False" SceneId="9" EffectId="-1">
   <Line CharacterId="7" Prio="0">
    <Sound Text="Can I pull the omnitool out?" FileName="cathandsimon_4_cath_backtogetomnitool_3_001_player_001" Timestamp="2014-11-09 12:27" />
   </Line>
   <Line CharacterId="16" Direction="(anxious)" Prio="0">
    <Sound Text="Wait, wait, wait... okay, just be sure to plug me in again at some point." EndPadding="-0.3" FileName="cathandsimon_4_cath_backtogetomnitool_3_002_catherineomnitool_001" Timestamp="2014-11-07 07:53" />
   </Line>
   <Line CharacterId="7" Prio="0">
    <Sound Text="You got it." EndPadding="-0.4" FileName="cathandsimon_4_cath_backtogetomnitool_3_003_player_001" Timestamp="2014-11-07 07:53" />
   </Line>
   <Line CharacterId="16" Prio="0" Callback="LineOver_Cath_BackToPickOmniToolUp">
    <Sound Text="Good luck, Simon." FileName="cathandsimon_4_cath_backtogetomnitool_3_004_catherineomnitool_001" Timestamp="2014-11-07 07:53" />
   </Line>
  </Subject>
  <Subject Name="3_Cath_CheckCityPlans" UseSingleRandomLine="False" SceneId="9" EffectId="-1">
